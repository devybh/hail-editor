{
  "name": "hail-editor",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "files": [
    "dist"
  ],
  "main": "./dist/hail-editor.umd.js",
  "module": "./dist/hail-editor.es.js",
  "types": "./dist/hail-editor.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hail-editor.es.js",
      "require": "./dist/hail-editor.umd.js"
    }
  },
  "devDependencies": {
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/eslint-plugin-tslint": "latest",
    "@typescript-eslint/parser": "latest",
    "@typescript-eslint/scope-manager": "latest",
    "@typescript-eslint/typescript-estree": "latest",
    "@typescript-eslint/utils": "latest",
    "eslint": "latest",
    "eslint-config-airbnb-base": "latest",
    "eslint-config-airbnb-typescript": "latest",
    "eslint-config-airbnb-typescript-prettier": "latest",
    "eslint-config-prettier": "latest",
    "eslint-define-config": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "json": "latest",
    "load-json-file": "latest",
    "prettier": "latest",
    "sass": "latest",
    "tslint": "latest",
    "type-fest": "latest",
    "typescript": "latest",
    "typescript-is": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vite-tsconfig-paths": "latest",
    "vitest": "latest"
  },
  "dependencies": {
    "lodash": "latest"
  },
  "packageManager": "yarn@3.2.2",
  "private": true,
  "version": "0.0.0",
  "description": "Modern Web Text/Contents Editor",
  "keywords": [
    "editor",
    "text editor"
  ],
  "homepage": "https://github.com/devybh/hail-editor",
  "bugs": {
    "url": "https://github.com/devybh/hail-editor/issues",
    "email": "devybh@gmail.com"
  },
  "author": {
    "name": "ByeongHwa Yoo",
    "email": "devybh@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devybh/hail-editor.git"
  }
}
